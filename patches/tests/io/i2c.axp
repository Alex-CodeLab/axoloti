   <obj type="script" sha="ba13a90be49d2157146d885c2cdf29e2c36cf457" name="script_1" x="80" y="240">
      <params/>
      <attribs>
         <text attributeName="script">
            <sText><![CDATA[
// demo serial transmission using I2C
uint8_t rxbuf[8];
uint8_t txbuf[8];
systime_t tmo = MS2ST(4);

while(!chThdShouldTerminate()){
  msg_t status = RDY_OK;
  txbuf[0] = 0x02;
  txbuf[1] = 0x03;
//  i2cAcquireBus(&I2CD1);
  status = i2cMasterTransmitTimeout(&I2CD1, 0x01, txbuf, 2, rxbuf, 0, tmo);
//  i2cReleaseBus(&I2CD1);
  chThdSleepMilliseconds(10);
}
]]></sText>
         </text>
      </attribs>
   </obj>
