<objdefs>
   <obj.normal id="sine lin" uuid="5ad3b6d0656a7407d1f09932117f538850ecf616" sha="1f2d05363eb787d5ea0d5c8394761f06256af740">
      <sDescription>Cheapest sine wave LFO, non-interpolated table</sDescription>
      <author>Johannes Taelman</author>
      <license>BSD</license>
      <inlets>
         <frac32 name="frequency" description="phase increment"/>
      </inlets>
      <outlets>
         <frac32.bipolar name="out" description="sine wave LFO, cheap and bad quality"/>
      </outlets>
      <displays/>
      <params/>
      <attribs/>
      <code.declaration><![CDATA[uint32_t Phase;]]></code.declaration>
      <code.init><![CDATA[Phase = 0;]]></code.init>
      <code.krate><![CDATA[Phase += inlet_frequency;
outlet_out= sinet[Phase>>22]<<12;]]></code.krate>
   </obj.normal>
</objdefs>